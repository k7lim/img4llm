{
  "name": "img4llm",
  "version": "0.1.0",
  "description": "Image optimization for LLM vision consumption",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    }
  },
  "types": "./lib/index.d.ts",
  "bin": {
    "img4llm": "lib/cli.cjs"
  },
  "files": [
    "README.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "unbuild",
    "postbuild": "node -e \"const fs=require('fs');const f='lib/cli.cjs';let c=fs.readFileSync(f,'utf8');if(c.startsWith('// #!')){c=c.slice(3)}if(!c.startsWith('#!')){c='#!/usr/bin/env node\\n'+c}fs.writeFileSync(f,c);fs.chmodSync(f,0o755)\"",
    "test": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "imagetracerjs": "^1.2.6",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^20.2.3",
    "jest": "^29.7.0",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "unbuild": "3.0.0-rc.7"
  }
}
